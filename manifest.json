{
    "manifest_version": 2,
    "name": "korail automator",
    "version": "0.1",
    "permissions": [
        "tabs",
        "notifications",
        "activeTab",
        "browsingData",
        "contextMenus",
        "storage",
        "alarms",
        "*://*/*"
        
      ],
    "content_scripts": [
        {
            "matches": [
            "http://www.letskorail.com/ebizprd/EbizPrdTicketPr21111_i1.do"
            ],
            "js": ["jquery-3.4.1.min.js","content.js"],
            "all_frames": true
        }    
    ],
    "browser_action": {
        "default_icon": "main-icon.png"
    },
    "icons": {
        "16": "main-icon.png" 
    },
    "background": {
        "scripts": ["background.js"]
    },
    "web_accessible_resources": [
        "favicon.ico",
        "btn_inq_tick_hack.png"
    ]
  }